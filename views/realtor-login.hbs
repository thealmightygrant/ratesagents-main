<div class="header clearfix">
  {{> page/nav-realtors}}
  <h3 class="text-muted">Rates and Agents</h3>
</div>

<h2 class="page-header">Realtor Account Login</h2>
<form method="post" action="/realtors/login">
  <div class="form-group">
    <label>Username</label>
    <input type="text" class="form-control" placeholder="janie-the-realtor">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="text" class="form-control">
  </div>
  <div class="form-group">
    <button type="submit" class="btn btn-default">Submit</button>
  </div>
</form>
