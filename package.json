{
  "name": "ratesagents-main",
  "version": "0.1.0",
  "description": "Login area and other important things",
  "main": "index.js",
  "scripts": {
    "compile-css-dev": "node-sass --include-path client_side/src/scss --output-style nested --indent-type tab --indent-width 4 client_side/src/scss/main.scss client_side/dist/css/main.css",
    "compile-css-prod": "node-sass --include-path client_side/src/scss --output-style compressed client_side/src/scss/main.scss client_side/dist/css/main.css",
    "autoprefix-css": "postcss --use autoprefixer --autoprefixer.browsers '> 5%, ie 9' client_side/dist/css/main.css -d client_side/dist/css",
    "build-css-prod": "npm run compile-css-prod && npm run autoprefix-css",
    "compress-images": "imagemin client_side/src/images/* -o client_side/dist/images",
    "compress-and-pack-js": "webpack --config ./config/webpack.config.js",
    "copy-fonts": "cp -rf client_side/src/fonts client_side/dist/",
    "mkdir-dist": "mkdir -p client_side/dist/js; mkdir -p client_side/dist/images; mkdir -p client_side/dist/css; mkdir -p client_side/dist/fonts;",
    "clean-dist": "rm -rf client_side/dist",
    "prep-dist": "npm run clean-dist; npm run mkdir-dist;",
    "build-dev": "npm run prep-dist; npm run copy-fonts; npm run compile-css-dev; npm run compress-and-pack-js; npm run compress-images",
    "build-prod": "npm run prep-dist; npm run copy-fonts; npm run build-css-prod; npm run compress-images; npm run compress-and-pack-js",
    "watch-css": "nodemon -e scss --watch client_side/src/scss -x \"npm run compile-css-dev\"",
    "watch-js": "nodemon -e js,hbs --watch client_side/src/js --watch index.js --watch utils --watch routes -x \"npm run compress-and-pack-js && node index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Grant Sherrick",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "imagemin-cli": "^3.0.0",
    "js-beautify": "^1.6.4",
    "jshint": "^2.9.3",
    "mocha": "^3.1.2",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "postcss-cli": "^2.6.0",
    "sequelize-cli": "^2.1.0",
    "tern": "^0.20.0",
    "uglify-js": "^2.7.3",
    "umzug": "^1.11.0"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-register": "^6.18.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "connect-session-sequelize": "^3.1.0",
    "convict": "^1.5.0",
    "cookie-session": "^2.0.0-alpha.1",
    "csurf": "^1.9.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.14.1",
    "express-validator": "2.21.0",
    "glob": "^7.1.1",
    "handlebars": "^4.0.5",
    "helmet": "^2.3.0",
    "jquery": "2.1.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.0",
    "material-design-icons": "^3.0.1",
    "materialize-css": "^0.97.8",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "sequelize": "^3.24.3",
    "webpack": "^1.13.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
